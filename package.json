{
  "dependencies": {
    "express": "^4.18.2",
    "gts": "^5.0.1"
  },
  "name": "fetch",
  "version": "1.0.0",
  "description": "take home assessment for fetch",
  "main": "server.js",
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": true
      }
    },
